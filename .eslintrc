{
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": ["error"],
		"no-use-before-define": 0,
		"import/no-unresolved": 0,
		"no-restricted-syntax": 0,
		"no-param-reassign": ["error", { "props": false }],
		"object-literal-sort-keys": false
	},
	"env": {
		"browser": true,
		"jasmine": true,
		"jest": true
	}
}
